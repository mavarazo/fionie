{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "type": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Clutch",
        "Necessaire",
        "Sac",
        "Handytasche",
        "Allerlei"
      ]
    },
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "isReserved": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "isSold": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "content": {
      "type": "richtext"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
